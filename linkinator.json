{
  "recurse": true,
  "skip": [
    "linkedin\\.com",
    "twitter\\.com",
    "facebook\\.com",
    "instagram\\.com",
    "youtube\\.com",
    "pinterest\\.com",
    "tiktok\\.com",
    "snapchat\\.com"
